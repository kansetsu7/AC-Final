<div><h2>Site Name</h2></div>
<div class="container">
  <div class="tab">
    
  </div>
  <div class="content">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Replies count</th>
          <th scope="col">Title</th>
          <th scope="col">Last replied at</th>
          <th scope="col">Viewed count</th>
        </tr>
      </thead>
      <tbody>
        <% @posts.each do |post| %>
          <tr>
            <td><%= post.replies_count %></td>
            <td><%= post.title %></td>
            <td><%= post.comments.latest_time %></td>
            <td><%= post.viewed_count %></td>
          </tr>          
        <% end %>
        
      </tbody>
    </table>
    <%= paginate @posts %>
  </div>
</div>

